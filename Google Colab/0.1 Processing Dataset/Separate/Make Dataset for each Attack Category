nate((train_blackbox_normal_data, train_blackbox_malicious_data))\n","# Shuffle normal traffic and malicious traffic\n","train_data_blackbox = shuffle(train_data_blackbox)\n","# Make dataset for Blackbox\n","blackbox_trainset=pd.DataFrame(data=train_data_blackbox,\n","                              index=[i for i in range(train_data_blackbox.shape[0])],\n","                              columns=columns)\n","\n","# Training set for GAN-Discriminator\n","discriminator_trainset=pd.DataFrame(data=train_discriminator_normal_data,\n","                                    index=[i for i in range(train_discriminator_normal_data.shape[0])],\n","                                    columns=columns)\n","\n","# Training set for GAN-Generator\n","generator_trainset=pd.DataFrame(data=train_generator_malicious_data,\n","                                index=[i for i in range(train_generator_malicious_data.shape[0])],\n","                                columns=columns)\n","# Testing set for GAN-Generator\n","generator_testset=pd.DataFrame(data=test_generator_malicious_data,\n","                                index=[i for i in range(test_generator_malicious_data.shape[0])],\n","                                columns=columns)"],"execution_count":0,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"pYrNspfUGIyI","colab_type":"text"},"source":["##**2.3. Kiểm tra số lượng record dữ liệu của bộ dữ liệu mới được tạo**\n","Kiểm tra tổng số lượng record của các bộ dữ liệu được tạo ra có bằng số record ở bộ dữ liệu gốc không"]},{"cell_type":"code","metadata":{"id":"BFI9WLikBqWd","colab_type":"code","outputId":"c739076d-e60a-4d31-897d-789b009ba06f","executionInfo":{"status":"ok","timestamp":1590949406675,"user_tz":-420,"elapsed":5878,"user":{"displayName":"Tien LE KHAC","photoUrl":"","userId":"18298916241916904857"}},"colab":{"base_uri":"https://localhost:8080/","height":272}},"source":["# Checking\n","print(\"{}\\nOrigin Trainset\".format(100*'='))\n","print(train_set.shape)\n","\n","pr